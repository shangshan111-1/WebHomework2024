{
  "version": 2,
  "builds": [
    {
      "src": "Astro/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "Astro/dist" }
    },
    {
      "src": "Django/djangoProject/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "50mb" }
    }
  ],
  "routes": [

    { "src": "/Astro/(.*)", "dest": "/Astro/dist/$1" },
    { "src": "/Django/(.*)", "dest": "/Django/djangoProject/wsgi.py" }

  ]

}

